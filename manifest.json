{
    "name": "SecureBrowser",
    "version": "1.0",
    "description": "Безопасный браузер с антивирусом и защитой приватности",
    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "storage",
        "tabs",
        "<all_urls>"
    ],
    "background": {
        "scripts": ["background.js"],
        "persistent": true
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "manifest_version": 2
}